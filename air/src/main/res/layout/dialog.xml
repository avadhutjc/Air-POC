<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <RelativeLayout
        android:id="@+id/rel1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageButton
            android:id="@+id/imageDialog"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10sp"
            android:layout_marginBottom="10dp"
            android:background="@color/white"
            android:src="@drawable/switch_avatar"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textDialog"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@+id/imageDialog"
            android:fontFamily="@font/poppins"
            android:text="Shushma Kumar"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textDialog2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/textDialog"
            android:layout_toEndOf="@+id/imageDialog"
            android:fontFamily="@font/poppins"
            android:text="Midwife"
            android:textColor="@color/grey"
            android:textSize="15sp" />

        <Button
            android:id="@+id/btn_Active"
            android:layout_width="90dp"
            android:layout_height="35dp"
            android:layout_below="@id/textDialog2"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/imageDialog"
            android:background="@drawable/btn_corner_dialog"
            android:backgroundTint="#1976D2"
            android:fontFamily="@font/poppins"
            android:foregroundGravity="center"
            android:gravity="center"
            android:text="Active"
            android:textColor="@color/white"
            android:textSize="11sp"
            android:textStyle="bold" />
        <!--        <com.gc.materialdesign.views.ButtonRectangle-->
        <!--            android:id="@+id/btn_Active"-->
        <!--            android:layout_width="wrap_content"-->
        <!--            android:layout_height="wrap_content"-->
        <!--            android:background="#1E88E5"-->
        <!--            android:text="Button" />-->
        <View
            android:id="@+id/divider_dialog"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_below="@id/btn_Active"
            android:layout_centerVertical="true"
            android:layout_marginTop="10dp"
            android:background="@drawable/divider" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rel1"
        android:layout_marginTop="10dp">

        <ImageButton
            android:id="@+id/imageDialog1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10sp"
            android:layout_marginTop="10sp"
            android:layout_marginEnd="10sp"
            android:layout_marginBottom="10sp"
            android:background="@color/white"
            android:src="@drawable/switch_avatar" />

        <TextView
            android:id="@+id/textDialog1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@+id/imageDialog1"
            android:fontFamily="@font/poppins"
            android:text="Savitri Rathod"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textDialog21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/textDialog1"
            android:layout_toEndOf="@+id/imageDialog1"
            android:fontFamily="@font/poppins"
            android:text="Medical Officer"
            android:textColor="@color/grey"
            android:textSize="15sp" />

        <View
            android:id="@+id/divider_dialog1"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_below="@id/textDialog21"
            android:layout_centerVertical="true"
            android:layout_marginTop="10dp"
            android:background="@drawable/divider" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/rel11"
        android:layout_marginTop="10dp">

        <ImageButton
            android:id="@+id/imageDialog11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10sp"
            android:layout_marginTop="10sp"
            android:layout_marginEnd="10sp"
            android:layout_marginBottom="10sp"
            android:background="@color/white"
            android:contentDescription="TODO"
            android:src="@drawable/j_switch_user" />


        <TextView
            android:id="@+id/textDialog11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_toEndOf="@+id/imageDialog11"
            android:fontFamily="@font/poppins"
            android:text="Jalaj Mathur"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textDialog211"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/textDialog11"
            android:layout_toEndOf="@+id/imageDialog11"
            android:fontFamily="@font/poppins"
            android:text="Birth attendant"
            android:textColor="@color/grey"
            android:textSize="15sp" />

        <View
            android:id="@+id/divider_dialog11"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_below="@id/textDialog211"
            android:layout_centerVertical="true"
            android:layout_marginTop="10dp"
            android:background="@drawable/divider" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rel1213"
        android:layout_width="wrap_content"
        android:layout_height="55dp"
        android:layout_below="@id/rel111"
        android:layout_centerInParent="true">

        <TextView
            android:id="@+id/btn_manage_user"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="15dp"
            android:backgroundTint="@color/white"
            android:fontFamily="@font/poppins_medium"
            android:gravity="center"
            android:text="Manage Users"
            android:textAllCaps="true"
            android:textColor="#0097A7"
            android:textSize="18sp" />
    </RelativeLayout>

</RelativeLayout>